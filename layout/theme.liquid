        --low-stock-text-color: {{ settings.low_stock_text_color.rgb }};
        --very-low-stock-text-color: {{ settings.very_low_stock_text_color.rgb }};
        --no-stock-text-color: {{ settings.no_stock_text_color.rgb }};
